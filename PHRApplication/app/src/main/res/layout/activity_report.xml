<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ReportActivity">


    <TextView
        android:id="@+id/report4"
        android:layout_width="88dp"
        android:layout_height="36dp"
        android:layout_marginStart="76dp"
        android:layout_marginTop="39dp"
        android:layout_marginEnd="307dp"
        android:text="Report"
        android:textSize="27sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <LinearLayout
        android:id="@+id/rectangle_1"
        android:layout_width="319dp"
        android:layout_height="175dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="46dp"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="46dp"
        android:background="@drawable/rectangle_1"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/report4">

        <LinearLayout
            android:layout_width="109dp"
            android:layout_height="match_parent"
            android:layout_marginLeft="22dp"
            android:layout_marginTop="33.5dp"
            android:layout_marginRight="21dp"
            android:layout_marginBottom="34.5dp"
            android:orientation="vertical">

            <TextView
                android:id="@+id/heart_rate"
                android:layout_width="79dp"
                android:layout_height="24dp"
                android:textSize="16sp"
                android:gravity="top"
                android:lineSpacingExtra="2sp"
                android:text="@string/heart_rate"
                android:textAppearance="@style/heart_rate"
                android:translationY="-0.85sp" />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/bpm"
                    android:layout_width="60dp"
                    android:layout_height="75dp"
                    android:gravity="top"
                    android:lineSpacingExtra="2sp"
                    android:text="96"
                    android:textStyle="bold"
                    android:textSize="50.91sp"
                    android:translationY="-1.2sp" />

                <TextView
                    android:layout_width="49dp"
                    android:layout_height="50dp"
                    android:gravity="top"
                    android:lineSpacingExtra="2sp"
                    android:text="bpm"
                    android:textSize="23sp"
                    android:translationY="-1.2sp"/>
            </LinearLayout>

        </LinearLayout>

        <ImageView
            android:id="@+id/imageView7"
            android:layout_width="147dp"
            android:layout_height="116dp"
            android:layout_marginLeft="21dp"
            android:layout_marginTop="29dp"
            android:layout_marginRight="20dp"
            android:layout_weight="1"
            app:srcCompat="@drawable/ekg" />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/linearLayout4"
        android:layout_width="320dp"
        android:layout_height="135dp"
        android:layout_marginStart="46dp"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="45dp"
        android:orientation="horizontal"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/rectangle_1">

        <LinearLayout
            android:layout_width="153dp"
            android:layout_height="match_parent"
            android:background="@drawable/bloodgroup"
            android:orientation="vertical">

            <ImageView
                android:id="@+id/imageView8"
                android:layout_width="32dp"
                android:layout_height="32.06dp"
                android:layout_marginLeft="22dp"
                android:layout_marginTop="17dp"
                app:srcCompat="@drawable/blood" />

            <TextView
                android:id="@+id/blood_group"
                android:layout_width="79dp"
                android:layout_height="16dp"
                android:layout_marginLeft="22dp"
                android:layout_marginTop="10dp"
                android:gravity="top"
                android:lineSpacingExtra="-3sp"
                android:text="Blood Group" />

            <TextView
                android:id="@+id/textView18"
                android:layout_width="38dp"
                android:layout_height="37dp"
                android:layout_marginLeft="22dp"
                android:layout_marginTop="4dp"
                android:text="A+"
                android:textStyle="bold"
                android:textSize="28sp" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="155dp"
            android:layout_height="match_parent"
            android:layout_marginLeft="12dp"
            android:background="@drawable/weight"
            android:orientation="vertical">

            <ImageView
                android:id="@+id/imageView9"
                android:layout_width="32dp"
                android:layout_height="32dp"
                android:layout_marginLeft="22dp"
                android:layout_marginTop="17dp"
                android:src="@drawable/weighticon" />

            <TextView
                android:id="@+id/textView17"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="22dp"
                android:layout_marginTop="10dp"
                android:text="Weight" />

            <TextView
                android:id="@+id/textView19"
                android:layout_width="70dp"
                android:layout_height="37dp"
                android:layout_marginLeft="22dp"
                android:layout_marginTop="4dp"
                android:text="80 kg"
                android:textStyle="bold"
                android:textSize="28sp" />


        </LinearLayout>
    </LinearLayout>

    <TextView
        android:id="@+id/textView20"
        android:layout_width="117dp"
        android:layout_height="22dp"
        android:layout_marginStart="46dp"
        android:layout_marginTop="27dp"
        android:textSize="17sp"
        android:text="Latest Report"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/linearLayout4" />

    <LinearLayout
        android:id="@+id/linearLayout5"
        android:layout_width="320dp"
        android:layout_height="242dp"
        android:layout_marginStart="45dp"
        android:layout_marginEnd="46dp"
        android:orientation="vertical"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView20">


        <LinearLayout
            android:layout_width="320dp"
            android:layout_height="93dp"
            android:layout_marginTop="16dp"
            android:background="@drawable/latestreport"
            android:orientation="horizontal">

            <ImageView
                android:id="@+id/imageView10"
                android:layout_width="64dp"
                android:layout_height="64dp"
                android:layout_marginLeft="11dp"
                android:layout_marginTop="14.82dp"
                android:layout_marginRight="16dp"
                android:layout_weight="1"
                android:background="@drawable/general" />

            <LinearLayout
                android:layout_width="128dp"
                android:layout_height="47dp"
                android:layout_marginLeft="16dp"
                android:layout_marginTop="14.82dp"
                android:layout_marginRight="101dp"
                android:layout_weight="1"
                android:orientation="vertical">

                <TextView
                    android:id="@+id/textView8"
                    android:layout_width="128dp"
                    android:layout_height="21dp"
                    android:layout_marginBottom="8dp"
                    android:text="General Health"
                    android:textSize="18dp"
                    android:textStyle="bold" />

                <TextView
                    android:id="@+id/textView7"
                    android:layout_width="60dp"
                    android:layout_height="18dp"
                    android:text="8 files" />
            </LinearLayout>
        </LinearLayout>
    </LinearLayout>

    <LinearLayout
        android:id="@+id/linearLayout3"
        android:layout_width="408dp"
        android:layout_height="48dp"
        android:layout_marginBottom="10dp"
        android:orientation="horizontal"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent">

        <LinearLayout
            android:id="@+id/home2"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:orientation="vertical">

            <ImageView
                android:id="@+id/imageView11"
                android:layout_width="match_parent"
                android:layout_height="24dp"
                android:layout_weight="1"
                android:background="#5EFFFFFF"
                app:srcCompat="@drawable/home2"
                tools:srcCompat="@drawable/home2" />

            <TextView
                android:id="@+id/textView5"
                android:layout_width="match_parent"
                android:layout_height="16dp"
                android:layout_marginTop="8dp"
                android:layout_weight="1"
                android:text="Home"
                android:textAlignment="center" />
        </LinearLayout>

        <LinearLayout
            android:id="@+id/schedul3"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:orientation="vertical">

            <ImageView
                android:id="@+id/imageView_schedule"
                android:layout_width="match_parent"
                android:layout_height="24dp"
                android:layout_weight="1"
                app:srcCompat="@drawable/schedule"
                tools:srcCompat="@drawable/schedule" />

            <TextView
                android:id="@+id/textView6"
                android:layout_width="match_parent"
                android:layout_height="16dp"
                android:layout_marginTop="8dp"
                android:layout_weight="1"
                android:text="Schedule"
                android:textAlignment="center" />
        </LinearLayout>

        <LinearLayout
            android:id="@+id/report8"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:orientation="vertical">

            <ImageView
                android:id="@+id/imageView13"
                android:layout_width="match_parent"
                android:layout_height="24dp"
                android:layout_weight="1"
                app:srcCompat="@drawable/report2"
                tools:srcCompat="@drawable/report2" />

            <TextView
                android:id="@+id/textView9"
                android:layout_width="match_parent"
                android:layout_height="16dp"
                android:layout_marginTop="8dp"
                android:layout_weight="1"
                android:text="Report"
                android:textAlignment="center" />
        </LinearLayout>

        <LinearLayout
            android:id="@+id/notify2"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:orientation="vertical">

            <ImageView
                android:id="@+id/imageView14"
                android:layout_width="match_parent"
                android:layout_height="24dp"
                android:layout_weight="1"
                app:srcCompat="@drawable/notifications"
                tools:srcCompat="@drawable/notifications" />

            <TextView
                android:id="@+id/textView10"
                android:layout_width="match_parent"
                android:layout_height="16dp"
                android:layout_marginTop="8dp"
                android:layout_weight="1"
                android:text="Notifications"
                android:textAlignment="center" />
        </LinearLayout>

    </LinearLayout>

</androidx.constraintlayout.widget.ConstraintLayout>