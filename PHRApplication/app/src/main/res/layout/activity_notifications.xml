<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".NotificationsActivity">

    <TextView
        android:id="@+id/notification"
        android:layout_width="161dp"
        android:layout_height="35dp"
        android:layout_marginStart="100dp"
        android:layout_marginTop="58dp"
        android:layout_marginEnd="300dp"
        android:text="Notifications"
        android:textSize="27sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <LinearLayout
        android:id="@+id/linearLayout5"
        android:layout_width="335dp"
        android:layout_height="643dp"
        android:layout_marginStart="28dp"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="46dp"
        android:orientation="vertical"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/notification">


        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="93dp"
            android:layout_marginTop="16dp"
            android:background="@drawable/latestreport"
            android:orientation="horizontal">

            <ImageView
                android:id="@+id/imageView10"
                android:layout_width="64dp"
                android:layout_height="64dp"
                android:layout_marginLeft="20dp"
                android:layout_marginTop="14.82dp"
                android:layout_weight="1"
                android:background="@drawable/calenderschedule" />

            <LinearLayout
                android:layout_width="213dp"
                android:layout_height="47dp"
                android:layout_marginLeft="17dp"
                android:layout_marginTop="20dp"
                android:layout_marginRight="40dp"
                android:layout_weight="1"
                android:orientation="vertical">

                <TextView
                    android:id="@+id/schedules"
                    android:layout_width="128dp"
                    android:layout_height="21dp"
                    android:layout_marginBottom="8dp"
                    android:text="3 Schedules!"
                    android:textSize="18dp"
                    android:textStyle="bold" />

                <TextView
                    android:id="@+id/textView7"
                    android:layout_width="match_parent"
                    android:layout_height="18dp"
                    android:text="Check your schedule today" />
            </LinearLayout>
        </LinearLayout>


        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="93dp"
            android:layout_marginTop="16dp"
            android:background="@drawable/latestreport"
            android:orientation="horizontal">

            <ImageView
                android:id="@+id/messagesimg"
                android:layout_width="64dp"
                android:layout_height="64dp"
                android:layout_marginLeft="20dp"
                android:layout_marginTop="14.82dp"
                android:layout_weight="1"
                android:background="@drawable/messagesnotifications" />

            <LinearLayout
                android:layout_width="213dp"
                android:layout_height="47dp"
                android:layout_marginLeft="17dp"
                android:layout_marginTop="20dp"
                android:layout_marginRight="40dp"
                android:layout_weight="1"
                android:orientation="vertical">

                <TextView
                    android:id="@+id/messages"
                    android:layout_width="128dp"
                    android:layout_height="21dp"
                    android:layout_marginBottom="8dp"
                    android:text="14 Messages"
                    android:textSize="18dp"
                    android:textStyle="bold" />

                <TextView
                    android:id="@+id/textView8"
                    android:layout_width="match_parent"
                    android:layout_height="18dp"
                    android:text="Check your messages today" />
            </LinearLayout>
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="93dp"
            android:layout_marginTop="16dp"
            android:background="@drawable/latestreport"
            android:orientation="horizontal">

            <ImageView
                android:id="@+id/medicineimg"
                android:layout_width="64dp"
                android:layout_height="64dp"
                android:layout_marginLeft="20dp"
                android:layout_marginTop="14.82dp"
                android:layout_weight="1"
                android:background="@drawable/medicinenotification" />

            <LinearLayout
                android:layout_width="213dp"
                android:layout_height="47dp"
                android:layout_marginLeft="17dp"
                android:layout_marginTop="20dp"
                android:layout_marginRight="40dp"
                android:layout_weight="1"
                android:orientation="vertical">

                <TextView
                    android:id="@+id/medicine"
                    android:layout_width="128dp"
                    android:layout_height="21dp"
                    android:layout_marginBottom="8dp"
                    android:text="Medicine"
                    android:textSize="18dp"
                    android:textStyle="bold" />

                <TextView
                    android:id="@+id/textView11"
                    android:layout_width="match_parent"
                    android:layout_height="18dp"
                    android:text="Check your medications today" />
            </LinearLayout>
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="93dp"
            android:layout_marginTop="16dp"
            android:background="@drawable/latestreport"
            android:orientation="horizontal">

            <ImageView
                android:id="@+id/vaccineimg"
                android:layout_width="64dp"
                android:layout_height="64dp"
                android:layout_marginLeft="20dp"
                android:layout_marginTop="14.82dp"
                android:layout_weight="1"
                android:background="@drawable/vaccinenotification" />

            <LinearLayout
                android:layout_width="213dp"
                android:layout_height="47dp"
                android:layout_marginLeft="17dp"
                android:layout_marginTop="20dp"
                android:layout_marginRight="40dp"
                android:layout_weight="1"
                android:orientation="vertical">

                <TextView
                    android:id="@+id/vaccineupdates"
                    android:layout_width="150dp"
                    android:layout_height="21dp"
                    android:layout_marginBottom="8dp"
                    android:text="Vaccine Updates"
                    android:textSize="18dp"
                    android:textStyle="bold" />

                <TextView
                    android:id="@+id/textView12"
                    android:layout_width="match_parent"
                    android:layout_height="18dp"
                    android:text="Check your vaccines today" />
            </LinearLayout>
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="93dp"
            android:layout_marginTop="16dp"
            android:background="@drawable/latestreport"
            android:orientation="horizontal">

            <ImageView
                android:id="@+id/appupdates"
                android:layout_width="64dp"
                android:layout_height="64dp"
                android:layout_marginLeft="20dp"
                android:layout_marginTop="14.82dp"
                android:layout_weight="1"
                android:background="@drawable/appupdates" />

            <LinearLayout
                android:layout_width="213dp"
                android:layout_height="47dp"
                android:layout_marginLeft="17dp"
                android:layout_marginTop="20dp"
                android:layout_marginRight="40dp"
                android:layout_weight="1"
                android:orientation="vertical">

                <TextView
                    android:id="@+id/applicationupdates"
                    android:layout_width="150dp"
                    android:layout_height="21dp"
                    android:layout_marginBottom="8dp"
                    android:text="App Updates"
                    android:textSize="18dp"
                    android:textStyle="bold" />

                <TextView
                    android:id="@+id/textView13"
                    android:layout_width="match_parent"
                    android:layout_height="18dp"
                    android:text="Check your latest updates today" />
            </LinearLayout>
        </LinearLayout>

    </LinearLayout>

    <LinearLayout
        android:id="@+id/linearLayout3"
        android:layout_width="408dp"
        android:layout_height="48dp"
        android:layout_marginBottom="10dp"
        android:orientation="horizontal"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent">

        <LinearLayout
            android:id="@+id/home4"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:orientation="vertical">

            <ImageView
                android:id="@+id/imageView11"
                android:layout_width="match_parent"
                android:layout_height="24dp"
                android:layout_weight="1"
                android:background="#5EFFFFFF"
                app:srcCompat="@drawable/home2"
                tools:srcCompat="@drawable/home2" />

            <TextView
                android:id="@+id/textView5"
                android:layout_width="match_parent"
                android:layout_height="16dp"
                android:layout_marginTop="8dp"
                android:layout_weight="1"
                android:text="Home"
                android:textAlignment="center" />
        </LinearLayout>

        <LinearLayout
            android:id="@+id/schedule3"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:orientation="vertical">

            <ImageView
                android:id="@+id/imageView_schedule"
                android:layout_width="match_parent"
                android:layout_height="24dp"
                android:layout_weight="1"
                app:srcCompat="@drawable/schedule"
                tools:srcCompat="@drawable/schedule" />

            <TextView
                android:id="@+id/textView6"
                android:layout_width="match_parent"
                android:layout_height="16dp"
                android:layout_marginTop="8dp"
                android:layout_weight="1"
                android:text="Schedule"
                android:textAlignment="center" />
        </LinearLayout>

        <LinearLayout
            android:id="@+id/report5"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:orientation="vertical">

            <ImageView
                android:id="@+id/imageView13"
                android:layout_width="match_parent"
                android:layout_height="24dp"
                android:layout_weight="1"
                app:srcCompat="@drawable/report"
                tools:srcCompat="@drawable/report" />

            <TextView
                android:id="@+id/textView9"
                android:layout_width="match_parent"
                android:layout_height="16dp"
                android:layout_marginTop="8dp"
                android:layout_weight="1"
                android:text="Report"
                android:textAlignment="center" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:orientation="vertical">

            <ImageView
                android:id="@+id/imageView14"
                android:layout_width="match_parent"
                android:layout_height="24dp"
                android:layout_weight="1"
                app:srcCompat="@drawable/notifications2"
                tools:srcCompat="@drawable/notifications2" />

            <TextView
                android:id="@+id/textView10"
                android:layout_width="match_parent"
                android:layout_height="16dp"
                android:layout_marginTop="8dp"
                android:layout_weight="1"
                android:text="Notifications"
                android:textAlignment="center" />
        </LinearLayout>

    </LinearLayout>
</androidx.constraintlayout.widget.ConstraintLayout>